								<cellspan>6,1</cellspan>
								<flag>wxALL</flag>
								<border>5</border>
								<object class="wxTextCtrl" name="txt_predefined">
									<style>wxHSCROLL|wxTE_DONTWRAP|wxTE_MULTILINE</style>
